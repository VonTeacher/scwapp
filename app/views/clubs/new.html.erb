<% provide(:title, 'Add Club') %>

<h2>Add a New Club</h2>

<%= form_for @club do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </p>
  <p>
    <%= f.submit "Add Club" %>
  </p>
<% end %>
